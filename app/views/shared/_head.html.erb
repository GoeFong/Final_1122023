 <head>
  <title>
    <%= content_for?(:title) ? yield(:title) : "Helper" %>
  </title>

  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <meta name="viewport" content="width=device-width, initial-scale=1">
  <%= stylesheet_link_tag  'application', 'data-turbolinks-track': 'reload' %>
  <%= stylesheet_link_tag  'actiontext', 'data-turbolinks-track': 'reload' %>
  <%= stylesheet_link_tag  'custom', 'data-turbolinks-track': 'reload' %>
  <%= stylesheet_link_tag  'https://unpkg.com/slim-select@latest/dist/slimselect.css', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag "application", defer: true %>
  <script src="https://cdn.jsdelivr.net/gh/alumuko/vanilla-datetimerange-picker@latest/dist/vanilla-datetimerange-picker.js"></script>
  <%#= javascript_include_tag 'https://js.stripe.com/v3/', 'data-turbolinks-track': 'reload' %>
</head>
